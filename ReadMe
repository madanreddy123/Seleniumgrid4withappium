
#### Go to the Folder Directory selenium from the terminal and run each of the cmd command mentioned in the txt file from the selenium folder

import java.util.regex.*;

public class DateFinder {
    public static void main(String[] args) {
        String text = "The event is scheduled on 2025-08-20.";

        // Regex for yyyy-MM-dd
        String regex = "\\b\\d{4}-\\d{2}-\\d{2}\\b";
        Pattern pattern = Pattern.compile(regex);
        Matcher matcher = pattern.matcher(text);

        if (matcher.find()) {
            System.out.println("Date found: " + matcher.group());
        } else {
            System.out.println("No date found");
        }
    }
}